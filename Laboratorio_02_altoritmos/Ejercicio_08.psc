Algoritmo Ejercicio_08
	Definir A,B,C,D,NM,P,NF Como Real;
	Escribir 'Escribir la primera nota';
	Leer A;
	Escribir 'Escribir la segunta nota';
	Leer B;
	Escribir 'Escribir la tercera nota';
	Leer C;
	NM <- 13;
	P <- (A+B+C)/3;
	NF <- (P*0.8)+(NM*0.2);
	Escribir 'La nota final es: ',NF;
FinAlgoritmo
